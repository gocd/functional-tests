    <pipelines group="stage-fail-and-cancel">
    <pipeline name="FS1">
      <materials>
        <git url="$git-one" materialName="git-one" dest="git-one" autoUpdate="true"/>
      </materials>
      <stage name="stage1">
        <approval type="manual"/>
        <jobs>
          <job name="job1">
            <tasks>
              <exec command="ls"/>
            </tasks>
          </job>
        </jobs>
      </stage>
      <stage name="stage2-manual">
        <approval type="manual"/>
        <jobs>
          <job name="job2">
            <tasks>
                <exec command="sleep" args="10"/>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
    <pipeline name="FS2" lockBehavior="none">
      <materials>
        <pipeline pipelineName="FS1" stageName="stage2-manual"/>
      </materials>
      <stage name="stage1">

        <jobs>
          <job name="job1">
            <tasks>
              <exec command="sleep" args="60"/>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
    <pipeline name="FS3">
      <materials>
        <pipeline pipelineName="FS1" stageName="stage1"/>
        <pipeline pipelineName="FS2" stageName="stage1"/>
      </materials>
      <stage name="stage1">
        <jobs>
          <job name="job1">
            <tasks>
              <exec command="ls"/>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
  </pipelines>

