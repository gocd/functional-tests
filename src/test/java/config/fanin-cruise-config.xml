            <ignore pattern="**/*"/>
          </filter>
        </git>
        <git url="$git-two" dest="git-two" materialName="git-two"/>
      </materials>
    </pipeline>
    <pipeline name="acceptance" template="autoStagePipelineTemplate">
      <materials>
