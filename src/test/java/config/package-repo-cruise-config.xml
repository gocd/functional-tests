        <jobs>
          <job name="defaultJob">
            <tasks>
              <exec command="ls"/>
            </tasks>
          </job>
        </jobs>
      </stage>
    </pipeline>
  
     <pipeline name="K3">
      <materials>
        <hg url="$hgurl" dest="hg" materialName="hg-k3"/>
        <package ref="CP1"/>
      </materials>
      <stage name="defaultStage">
        <approval type="manual"/>
        <jobs>
          <job name="defaultJob">
            <tasks>
              <exec command="ls"/>
            </tasks>
            <artifacts>
              <artifact type="test" src="hg/dev/pass.xml" dest="pass"/>
            </artifacts>
          </job>
        </jobs>
      </stage>
    </pipeline>
    
    <pipeline name="K4">
      <materials>
        <hg url="$hgurl" dest="hg" materialName="hg-k4"/>
        <package ref="CP1"/>
      </materials>
      <stage name="defaultStage">
        <approval type="manual"/>
        <jobs>
          <job name="defaultJob">
            <tasks>
              <exec command="ls123"/>
            </tasks>
            <artifacts>
              <artifact type="test" src="hg/dev/fail.xml" dest="fail"/>
            </artifacts>
          </job>
        </jobs>
      </stage>
    </pipeline>
  
  </pipelines>
  
  <pipelines group="fanin-advanced">
    <pipeline name="fanin_a" template="fanin_a-template">
